<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.servicemusic"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
	<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.activity.MusicListActivity"></activity>
        <activity android:name="com.example.activity.LoginActivity"></activity>
        <activity android:name="com.example.activity.RegiserActivity"></activity>
        <activity android:name="com.example.activity.GridViewActicity"></activity>
        <activity android:name="com.example.activity.OnlineMusicActivity"></activity>
        
        <service android:name="com.example.service.MusicService"></service>
    </application>

</manifest>